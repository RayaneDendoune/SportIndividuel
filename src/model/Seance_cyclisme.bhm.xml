<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Seance_cyclisme" table="Seance_cyclisme">
        <id name="id_seance_cyclisme" column="id_seance_cyclisme">
            <!-- <generator class="increment"></generator> -->
        </id>

        <property name="niveau_activite_physique" column="niveau_activite_physique"></property>
        <property name="poids" column="poids"></property>
