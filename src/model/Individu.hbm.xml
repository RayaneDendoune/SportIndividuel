<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Individu" table="Individu">
        <id name="id" column="id">
        </id>

        <property name="nom" column="nom"></property>
        <property name="prenom" column="prenom"></property>
        <property name="mdp" column="mdp"></property>
        <property name="sexe" column="sexe"></property>
        <property name="age" column="age"></property>
        <property name="poids" column="poids"></property>
        <property name="taille" column="taille"></property>
        <property name="elo" column="elo"></property>
        <property name="frequence_jeu" column="frequence_jeu"></property>
    </class>
</hibernate-mapping>